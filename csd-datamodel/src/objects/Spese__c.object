<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>MaxCosto</fullName>
        <active>true</active>
        <errorConditionFormula>Costo__c &gt;9999.99</errorConditionFormula>
        <errorDisplayField>Costo__c</errorDisplayField>
        <errorMessage>Cifra troppo alta, riprova</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MinCosto</fullName>
        <active>true</active>
        <errorConditionFormula>Costo__c &lt; 1</errorConditionFormula>
        <errorDisplayField>Costo__c</errorDisplayField>
        <errorMessage>Prezzo non valido</errorMessage>
    </validationRules>
</CustomObject>
